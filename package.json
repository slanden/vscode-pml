{
	"name": "pdml-pml",
  "displayName": "PDML Vocabulary: PML",
	"description": "Practical Markup Language support",
  "icon": "dist/extension-icon.png",
	"publisher": "shelby-landen",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/slanden/vscode-pdml.git/pml"
	},
	"engines": {
		"vscode": "^1.86.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"pdml",
		"pml",
		"practical",
		"markup"
	],
	"main": "dist/main.cjs",
	"type": "commonjs",
	"contributes": {
		"languages": [
			{
				"id": "pdml",
				"aliases": [
					"Practical Markup Language",
					"pml"
				],
				"extensions": [
					".pml"
				]
			}
		]
	},
	"activationEvents": [
		"onLanguage:pdml"
	],
	"extensionDependencies": [
		"shelby-landen.pdml"
	],
	"dependencies": {
		"vscode-languageserver": "^9.0.1"
	},
	"devDependencies": {
		"@bytecodealliance/jco": "^1.12.0",
		"@types/vscode": "^1.75.1",
		"rolldown": "^1.0.0-beta.31"
	},
	"scripts": {
		"build": "rm -rf dist/* && rolldown --dir dist main.cjs --format cjs --entry-file-names main.cjs && cp ../extension-icon.png dist",
    "set-main-dev": "sed -i 's|\"main\": .*|\"main\": \"main.cjs\",|' package.json",
    "set-main-prod": "sed -i 's|\"main\": .*|\"main\": \"dist/main.cjs\",|' package.json",
    "vscode:prepublish": "npm run build"
	}
}
